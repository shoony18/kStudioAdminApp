                    <img id = "myimg1" style="height:200px; width:200px; background:gray;"><br>
                    <img id = "myimg2" style="height:200px; width:200px; background:gray;"><br>
                    画像1<input type="file" id="files1" multiple /><br>
                    画像2<input type="file" id="files2" multiple /><br>
<!--                    <button id ="select">画像を選択</button>-->
<!--                    <button id ="send">画像をアップロード</button>-->
                </div>
              </div>
                
                
                <button id = "backToShow" class="btn btn-secondary" type="button" data-toggle="collapse" data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample">
                    戻る
                </button>
                <button id = "applyShowCreateAnswer_bq" class="btn btn-secondary"  data-toggle="collapse" data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample" >
                    画像アップロード
                </button>
                <button id = "applyShowCreateAnswer" class="btn btn-secondary"  data-toggle="collapse" data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample" onclick="alert(); return false;">
                    回答送信
                </button>
              <br>
              <br>
                                    
