<div class="wrapper ">
    <div class="main-panel">

<!--      <div class="content">-->
        <div class="container-fluid" id="applyIndexArea">
          <div class="row">
            <div class="col-md-12">
              <div class="card">
                <div class="card-header card-header-secondary bg-secondary">
                  <h5 class="card-title ">選択された申込情報</h5>
                </div>
                <div class="card-body" id="applyShowTbody">
                </div>
              </div>
              <div class="card">
                <div class="card-header card-header-danger">
                  <h5 class="card-title ">回答</h5>
<!--                    <img src = "/assets/answerToApply.png">                    -->
                </div>
                  
                <div class="card-body">
                    <form id="applyShowAnswer">
                        <button id = "analyticsButton" class="btn btn-secondary" type="button" data-toggle="collapse" data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample" onclick="alert2(); return false;">
                            自動解析する
                        </button><br>
                    </form>
                </div>

                <div class="card-body">
                  <div class="btn btn-secondary disabled" style ="padding-bottom: 10px;">サマリースコア</div>
                  <div class="table-responsive">
                      総合得点：<%= @totalScore %>点
                    <table class="table">
                      <thead class=" text-secondary">
                        <th>
                          #
                        </th>
                        <th>
                          評価軸
                        </th>
                        <th>
                          点数
                        </th>
                      </thead>
                      <tbody id="applyTbody">          
                          <% (0..@anaCriteriaTitle.count-1).each do |i| %>
                          <tr>
                              <td><%= String(i+1) %></td>
                              <td><textarea id = "anaCriteriaTitle<%= String(i) %>"><%= @anaCriteriaTitle[i] %></textarea></td>
                              <td><textarea id = "criteriaScore<%= String(i) %>"><%= @criteriaScore[i] %></textarea></td>
                          </tr>
                          <% end %>
                      </tbody>
                    </table>

                  </div>
                    
                  <div class="btn btn-secondary disabled" style ="padding-bottom: 10px;">詳細スコア</div>
                  <div class="table-responsive">
                    <table class="table">
                      <thead class=" text-secondary">
                        <th>
                          #
                        </th>
                        <th>
                          解析ポイント
                        </th>
                        <th>
                          range_start(始点)
                        </th>
                        <th>
                          range_end(終点)
                        </th>
                        <th>
                          角度
                        </th>
                        <th>
                          差異
                        </th>
                        <th>
                          点数
                        </th>
                        <th>
                          FBフラグ
                        </th>
                        <th>
                          フィードバック
                        </th>
                      </thead>
                      <tbody id="applyTbody">          
                          <% (0..@anaPointID.count-1).each do |i| %>
                          <tr id="applyTr<%= String(i) %>">
                              <td><%= String(i+1) %></td>
                              <td><textarea id = "anaPointID<%= String(i) %>"><%= @anaPointID[i] %></textarea></td>
                              <td><textarea id = "range_start<%= String(i) %>"><%= @range_start[i] %></textarea></td>
                              <td><textarea id = "range_end<%= String(i) %>"><%= @range_end[i] %></textarea></td>
                              <td><textarea id = "angleValue<%= String(i) %>"><%= @angleValue[i] %></textarea></td>
                              <td><textarea id = "angleValueDiff<%= String(i) %>"><%= @angleValueDiff[i] %></textarea></td>
                              <td><textarea id = "detailScore<%= String(i) %>"><%= @detailScore[i] %></textarea></td>
                              <td><textarea id = "fbFlag<%= String(i) %>"><%= @fbFlag[i] %></textarea></td>
                              <td><textarea id = "fbContent<%= String(i) %>"><%= @fbContent[i] %></textarea></td>
<!--                              <td><textarea id = "fbContent%= String(i) %>"></textarea></td>-->
                          </tr>
                          <% end %>
                      </tbody>
                    </table>

                  </div>
<!--
                  <div id = "anaImg" class="btn btn-secondary disabled" style ="padding-bottom: 10px;">解析画像</div><br><br>
                    <div id = "myimg1_div" style="height:200px; width:200px; background:gray;">
                        <img id = "myimg1" style="height:200px; width:200px; background:gray;">
                    </div>
                    <div id = "myimg2_div" style="height:200px; width:200px; background:gray;">
                        <img id = "myimg2" style="height:200px; width:200px; background:gray;"> 
                    </div>
                    画像1<input type="file" id="files" multiple /><br>
                    画像2<input type="file" id="files2" multiple /><br>
-->

