<!--      <div class="content">-->
        <div class="container-fluid" id="applyIndexArea">
          <div class="row">
            <div class="col-md-12">
              <div class="card">
                <div class="card-header card-header-secondary bg-secondary">
                  <h5 class="card-title ">選択された申込情報</h5>
                </div>
                    <table class="table">
                      <thead class=" text-secondary">
                        <th>
                          #
                        </th>
                        <th>
                          解析ポイント
                        </th>
                        <th>
                          フィードバック（良い点）
                        </th>
                        <th>
                          フィードバック（改善点1）
                        </th>
                        <th>
                          フィードバック（改善点2）
                        </th>
                        <th>
                          range_start
                        </th>
                        <th>
                            range_end
                        </th>
                      </thead>
                      <tbody id="applyTbody">          
                          <% (0..@anaPointID.count-1).each do |i| %>
                          <tr id="applyTr<%= String(i) %>">
                              <td><%= String(i+1) %></td>
                              <td><textarea id = "anaPointID_<%= String(i) %>"></textarea></td>
                              <td><textarea id = "fb0_<%= String(i) %>"></textarea></td>
                              <td><textarea id = "fb1_<%= String(i) %>"></textarea></td>
                              <td><textarea id = "fb2_<%= String(i) %>"></textarea></td>
                              <td><textarea id = "range_start_<%= String(i) %>"></textarea></td>
                              <td><textarea id = "range_end_<%= String(i) %>"></textarea></td>
                          </tr>
                          <% end %>
                      </tbody>
                    </table>
              </div>
              <div class="card">              
                
                <button id = "backToShow" class="btn btn-secondary" type="button" data-toggle="collapse" data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample">
                    戻る
                </button>
                <button id = "applyShowCreateAnswer" class="btn btn-secondary" type="button" data-toggle="collapse" data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample" onclick="alert4(); return false;">
                    送信
                </button>
              <br>
              <br>
                                    
